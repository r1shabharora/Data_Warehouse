      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT Calendar_Dim.Year_, CustomerEmployee_Fact.SalesAmount_SUM, CustomerEmployee_Fact.SalesAmount_AVG, CustomerEmployee_Fact.TransactionCount, Calendar_Dim.Quarter_, Employee_Dim.FullName
FROM     Calendar_Dim INNER JOIN
                  CustomerEmployee_Fact ON Calendar_Dim.CalendarKey = CustomerEmployee_Fact.CalendarKey INNER JOIN
                  Employee_Dim ON CustomerEmployee_Fact.EmployeeKey = Employee_Dim.EmployeeKey</CommandText>
